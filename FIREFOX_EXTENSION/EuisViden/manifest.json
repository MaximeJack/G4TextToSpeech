{

  "description": "Extension pour les malvoyants et non-voyants",
  "manifest_version": 2,
  "name": "EiusViden",
  "version": "1.0",
  "icons": {
    "48": "icons/border-48.png"
  },
  "background": { 
    "scripts":["include/jquery/jquery.js", "Loupe/LoupeBackground.js"]
   },
   "content_scripts": [ {
    "js": [
        "include/jquery/jquery.js", 
        "include/jqueryAddExtension/zoom.js", 
        "include/html2canvas/html2canvas.js", 
        "Loupe/LoupeContent.js", 
        "Audio.js", 
        "AideNav.js"
    ],
    "css":["include/jqueryAddExtension/zoom.css"],
    "matches": [ "<all_urls>"]
  }],
   "permissions": [ "<all_urls>"],
   
  "browser_action": {
    "default_icon": "icons/border-48.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  }

}
